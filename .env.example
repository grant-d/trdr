# Alpaca API credentials
# Get from: https://app.alpaca.markets/paper/dashboard/overview
ALPACA_API_KEY=your_api_key_here
ALPACA_SECRET_KEY=your_secret_key_here

# PAPER
ALPACA_MODE=paper
ALPACA_PAPER_API_KEY=your_paper_api_key_here
ALPACA_PAPER_API_SECRET=your_paper_api_secret_here

# LIVE
export LIVE_SYMBOL=ETH/USD
export LIVE_TIMEFRAME=5m
export LIVE_POLL_INTERVAL=30  # Seconds

# Paper trading endpoint (default)
# ALPACA_BASE_URL=https://paper-api.alpaca.markets

# Symbol/Timeframe are code-driven in sica_bench.py
