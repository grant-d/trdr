{
  "input": {
    "type": "provider",
    "provider": "alpaca",
    "symbols": [
      "SPY"
    ],
    "timeframe": "1h",
    "duration": "30d"
  },
  "output": {
    "path": "./output/spy-fractional-diff.csv",
    "format": "csv",
    "overwrite": true
  },
  "transformations": [
    {
      "type": "missingValues",
      "disabled": false,
      "params": {
        "tx": [
          {
            "in": "open",
            "out": "open",
            "strategy": "forward"
          },
          {
            "in": "high",
            "out": "high",
            "strategy": "forward"
          },
          {
            "in": "low",
            "out": "low",
            "strategy": "forward"
          },
          {
            "in": "close",
            "out": "close",
            "strategy": "forward"
          },
          {
            "in": "volume",
            "out": "volume",
            "strategy": "forward"
          }
        ]
      }
    },
    {
      "type": "fractionalDiff",
      "disabled": false,
      "params": {
        "tx": {
          "in": "close",
          "out": "close_fd"
        },
        "d": 0.3,
        "maxWeights": 50,
        "minWeight": 0.001
      }
    }
  ],
  "options": {
    "chunkSize": 1000,
    "continueOnError": true,
    "maxErrors": 10,
    "showProgress": true
  },
  "metadata": {
    "name": "SPY Fractional Differentiation Example",
    "version": "1.0.0",
    "description": "Demonstrates fractional differentiation on SPY hourly data to achieve stationarity while preserving memory",
    "author": "Trading System",
    "created": "2025-01-06T00:00:00Z"
  }
}