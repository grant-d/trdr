{
  "input": {
    "type": "provider",
    "provider": "alpaca",
    "symbols": ["AAPL"],
    "timeframe": "1d",
    "duration": "1y"
  },
  "transformations": [
    {
      "type": "logReturns",
      "enabled": true,
      "params": {
        "description": "Calculate log returns for all OHLCV columns"
      }
    },
    {
      "type": "zScore",
      "enabled": true,
      "params": {
        "description": "Z-score normalization of all columns",
        "windowSize": 20
      }
    },
    {
      "type": "minMax",
      "enabled": true,
      "params": {
        "description": "Min-max normalization with 10-bar window",
        "windowSize": 10,
        "targetMin": 0,
        "targetMax": 1
      }
    }
  ],
  "output": {
    "path": "./data/alpaca-aapl-1d.jsonl",
    "format": "jsonl",
    "overwrite": true
  },
  "options": {
    "chunkSize": 1000,
    "continueOnError": true,
    "maxErrors": 10,
    "showProgress": true,
    "validateData": true
  },
  "metadata": {
    "name": "Alpaca AAPL Historical Data",
    "version": "1.0.0",
    "description": "Fetches 30 days of daily AAPL data from Alpaca",
    "created": "2025-01-05T00:00:00Z"
  }
}