{
  "input": {
    "type": "file",
    "format": "csv",
    "path": "./data/BTCUSD-feed.csv",
    "columnMapping": {
      "timestamp": "Date",
      "open": "Open",
      "high": "High",
      "low": "Low",
      "close": "Close",
      "volume": "Volume"
    }
  },
  "output": {
    "path": "./data/output.jsonl",
    "format": "jsonl",
    "overwrite": true
  },
  "transformations": [
    {
      "type": "logReturns",
      "disabled": false,
      "params": {
        "tx": {
          "in": "close",
          "out": "returns"
        }
      }
    },
    {
      "type": "zScore",
      "disabled": false,
      "params": {
        "tx": [
          {
            "in": "close",
            "out": "close_zscore"
          }
        ],
        "windowSize": 20
      }
    }
  ],
  "options": {
    "chunkSize": 1000,
    "continueOnError": true,
    "maxErrors": 10,
    "showProgress": true
  },
  "metadata": {
    "name": "BTC Price Analysis",
    "version": "1.0.0",
    "description": "Analyzes BTC price data with log returns and z-score normalization",
    "created": "2024-01-01T00:00:00Z"
  }
}