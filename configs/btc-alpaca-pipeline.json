{
  "input": {
    "type": "provider",
    "provider": "alpaca",
    "symbols": [
      "BTC/USD"
    ],
    "timeframe": "4h",
    "duration": "6M"
  },
  "output": {
    "path": "./output/btc-alpaca-1M.csv",
    "format": "csv",
    "overwrite": true
  },
  "transformations": [
    {
      "type": "missingValues",
      "disabled": false,
      "params": {
        "tx": [
          {
            "in": "open",
            "out": "open",
            "strategy": "forward"
          },
          {
            "in": "high",
            "out": "high",
            "strategy": "forward"
          },
          {
            "in": "low",
            "out": "low",
            "strategy": "forward"
          },
          {
            "in": "close",
            "out": "close",
            "strategy": "forward"
          },
          {
            "in": "volume",
            "out": "volume",
            "strategy": "forward"
          }
        ]
      }
    },
    {
      "type": "fractionalDiff",
      "disabled": false,
      "params": {
        "tx": {
          "in": "close",
          "out": "close_fd"
        },
        "d": 0.3
      }
    },
    {
      "type": "zScore",
      "disabled": false,
      "params": {
        "tx": [
          {
            "in": "close",
            "out": "close_zscore"
          }
        ],
        "windowSize": 29
      }
    },
    {
      "type": "rsi",
      "disabled": false,
      "params": {
        "tx": {
          "in": "close",
          "out": "rsi",
          "window": 14
        }
      }
    },
    {
      "type": "map",
      "disabled": false,
      "params": {
        "tx": {
          "in": "close",
          "out": "adj_close"
        }
      }
    },
    {
      "type": "lorentzianDistance",
      "disabled": false,
      "params": {
        "tx": {
          "in": [
            "open",
            "high",
            "low",
            "close",
            "volume"
          ],
          "cFactor": 1,
          "threshold": 50
        }
      }
    }
  ],
  "options": {
    "chunkSize": 1000,
    "continueOnError": true,
    "maxErrors": 10,
    "showProgress": true
  },
  "metadata": {
    "name": "BTC Lorentzian Distance Analysis",
    "version": "1.0.0",
    "description": "Fetches 1 year of daily BTC data from Alpaca, applies fractional differentiation, z-score normalization, and generates Lorentzian distance bars",
    "author": "Trading System",
    "created": "2025-01-06T00:00:00Z"
  }
}